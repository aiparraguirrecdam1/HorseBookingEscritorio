<Window x:Class="EscritorioHorseBooking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EscritorioHorseBooking"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="email" Width="150" Height="30" VerticalAlignment="Top"
                 Margin="20,20,20,20"/>

        <TextBox x:Name="contrasena" Width="150" Height="30" VerticalAlignment="Top"
                 Margin="20,80,20,20"/>
        
        <Button x:Name="registrar" Content="Registrar" Click="registrar_Click"
                Width="150" Height="50"/>
    </Grid>
</Window>
