<Window x:Class="EscritorioHorseBooking.Chat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EscritorioHorseBooking"
        mc:Ignorable="d"
        Title="Chat" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Menu DockPanel.Dock="Top" Background="#7BB8B1" Foreground="White">
            <MenuItem Header="Inicio" Click="Inicio_Click"/>
            <MenuItem Header="Novedades" Click="Novedades_Click"/>
            <MenuItem Header="Clases" Click="Clases_Click"/>
            <MenuItem Header="Chat" Foreground="Black" Background="AliceBlue"/>
            <MenuItem Header="Perfil" Click="PerfilUsuario_Click"/>
        </Menu>
    </Grid>
</Window>
